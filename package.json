{
  "name": "react-js-pagination",
  "version": "3.0.2",
  "description": "Simple, easy to use component for pagination. Compatible with bootstrap paginator stylesheets",
  "main": "./dist/Pagination.js",
  "scripts": {
    "clean": "rimraf dist",
    "clean-example": "rimraf src/example/dist",
    "build": "npm run clean && cross-env NODE_ENV=production node build.js",
    "build-example": "npm run clean-example && npm run build:webpack",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "start": "node devServer.js",
    "lint": "eslint 'src/components/**/*.js'",
    "test": "mocha --compilers js:babel-register --require ./test-setup.js 'src/**/*-test.js'",
    "validate": "npm-run-all lint test",
    "test:watch": "mocha -w --compilers js:babel-register --require ./test-setup.js 'src/**/*-test.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vayser/react-js-pagination"
  },
  "keywords": [
    "react",
    "reactjs",
    "pagination",
    "paginator",
    "bootstrap",
    "reload",
    "webpack",
    "babel",
    "react-transform",
    "react-component"
  ],
  "author": "Sergey Vayser <wice24@gmail.com> (http://github.com/vayser)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/vayser/react-js-pagination/issues"
  },
  "homepage": "https://github.com/vayser/react-js-pagination",
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.8",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.9",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "bootstrap": "^3.3.5",
    "chai": "^3.5.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.19.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "3.10.2",
    "eslint-plugin-react": "6.7.1",
    "expect": "^1.14.0",
    "express": "^4.13.3",
    "file-loader": "^0.8.4",
    "jsdom": "^8.0.4",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.7.4",
    "react-addons-test-utils": "15.x.x - 16.x.x",
    "react-dom": "15.x.x - 16.x.x",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-syntax-highlighter": "^5.7.0",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.4.3",
    "style-loader": "^0.12.4",
    "uglify-js": "^3.1.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "paginator": "^1.0.0",
    "prop-types": "15.x.x - 16.x.x",
    "react": "15.x.x - 16.x.x"
  }
}
